{
  "name": "eslint-config-gojutin",
  "version": "0.1.5",
  "description": "ESLint configuration for React/TypeScript projects with a functional flavor.",
  "author": "Justin Wilkerson <gojutin@gmail.com>",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write *.js",
    "lint": "eslint *.js",
    "test": "npm run lint",
    "release": "np --no-publish"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:gojutin/eslint-config-gojutin.git"
  },
  "engines": {
    "node": "^10.12.0 || >=12.0.0"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gojutin/eslint-config-gojutin/issues"
  },
  "homepage": "https://github.com/gojutin/eslint-config-gojutin#readme",
  "dependencies": {
    "@babel/eslint-parser": "^7.15.0",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.9.0",
    "eslint-plugin-functional": "^3.6.0",
    "eslint-plugin-immutable": ">=1.0.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-standard": "^4.1.0"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "javascript",
    "react",
    "typescript",
    "styleguide"
  ],
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "eslint": "^7.32.0",
    "husky": "^7.0.1",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
